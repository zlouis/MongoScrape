<div>
    <h1 class="card z-depth-3 center-align">The latest in the news!</h1>
    <div class="row">
      <div class="col s3"></div>
      <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' to get articles!</h5>
    </div>
</div>

<div class="row">
  <div class="col s2"></div>
  <div class ="col s8 left-align">
    {{#each article}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        {{!-- goes to article.handlebars --}}
        <p>Read article <a href="/readArticle/{{this.id}}">here</a></p>
      </div>
    {{/each}}
  </div>
</div>